<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <RulePackDef>
        <defName>AstroEmpireNameUtility</defName>
        <rulePack>
            <rulesFiles>
                <li>LOF_Noun_Prefix_Sign->Names/Noun/AS_Noun_Prefix_Sign</li>
                <li>LOF_Noun_Prefix_Stellar->Names/Noun/AS_Noun_Prefix_Stellar</li>
                <li>LOF_LastName_StellarStyle->Names/AS_LastName_StellarStyle</li>
            </rulesFiles>
            <rulesStrings>
                <li>LOF_FirstName->[AS_Noun_Prefix_Sign]</li>
                <li>LOF_FirstName->[AS_Noun_Prefix_Stellar]</li>
                <li>LOF_LastName->[AS_LastName_StellarStyle]</li>
            </rulesStrings>
        </rulePack>
    </RulePackDef>

    <RulePackDef>
        <defName>NamerPerson_AstroEmpire</defName>
        <include>
            <li>AstroEmpireNameUtility</li>
        </include>
        <rulePack>
            <rulesStrings>
                <li>r_name(p=4)->[AS_FirstName] '[AS_LastName]' [AS_LastName]</li>
                <li>r_name(p=2)->[AS_FirstName] [AS_LastName]</li>
                <li>firstName(p=1)->[AS_FirstName]</li>
                <li>nickName(p=1)->[AS_LastName]</li>
                <li>lastName(p=1)->[AS_LastName]</li>
            </rulesStrings>
        </rulePack>
    </RulePackDef>

