    <ThingDef ParentName="Astro_ApparelBase">
        <defName>LOF_Equip_EmpactPack</defName>
        <tickerType>Normal</tickerType>
        <label>&lt;color=#00AEEF&gt;"AMA-2" EMP冲击背包&lt;/color&gt;</label>
        <description>投射在地面上会产生持续的emp脉冲，范围较大且放置距离较长</description>
        <graphicData>
            <texPath>Things/Apparel/LOF_EmpactPack/EmpactPack</texPath>
            <graphicClass>Graphic_Single</graphicClass>
        </graphicData>
        <uiIconScale>1.5</uiIconScale>
        <!--占位,原版数值-->
        <statBases>
            <MaxHitPoints>80</MaxHitPoints>
            <WorkToMake>4000</WorkToMake>
            <Mass>3</Mass>
            <PackRadius>4.9</PackRadius>
            <EquipDelay>2</EquipDelay>
        </statBases>
        <costList>
            <LOF_Thing_StarlightIron>20</LOF_Thing_StarlightIron>
            <ComponentIndustrial>4</ComponentIndustrial>
        </costList>
        <comps>
            <li Class="AKA_Ability.TCP_AKATracker">
                <trackerGenProp>
                    <abilities>
                        <li>LOF_Ability_DeployEMP</li>
                    </abilities>
                </trackerGenProp>
            </li>
        </comps>
        <apparel>
            <renderSkipFlags>
                <li>None</li>
            </renderSkipFlags>
            <renderNodeProperties>
                <li>
                    <nodeClass>Astrologer.PawnRenderNode_Apparel</nodeClass>
                    <texPath>Things/Apparel/LOF_EmpactPack/EmpactPack</texPath>
                    <baseLayer>0</baseLayer>
                    <parentTagDef>Body</parentTagDef>
                    <pawnType>Any</pawnType>
                    <drawData>
                        <dataEast>
                            <layer>90</layer>
                        </dataEast>
                        <dataNorth>
                            <layer>90</layer>
                        </dataNorth>
                        <dataWest>
                            <layer>90</layer>
                        </dataWest>
                    </drawData>
                    <visibleFacing>
                        <li>0</li>
                        <li>1</li>
                        <li>3</li>
                    </visibleFacing>
                </li>
                <li>
                    <nodeClass>Astrologer.PawnRenderNode_Apparel</nodeClass>
                    <texPath>Things/Apparel/LOF_EmpactPack/EMPactPackLight</texPath>
                    <baseLayer>0</baseLayer>
                    <parentTagDef>Body</parentTagDef>
                    <pawnType>Any</pawnType>
                    <drawData>
                        <dataEast>
                            <layer>91</layer>
                        </dataEast>
                        <dataNorth>
                            <layer>91</layer>
                        </dataNorth>
                        <dataWest>
                            <layer>91</layer>
                        </dataWest>
                    </drawData>
                    <visibleFacing>
                        <li>0</li>
                        <li>1</li>
                        <li>3</li>
                    </visibleFacing>
                </li>
            </renderNodeProperties>
            <countsAsClothingForNudity>false</countsAsClothingForNudity>
            <developmentalStageFilter>Child, Adult</developmentalStageFilter>
            <bodyPartGroups>
                <li>Waist</li>
            </bodyPartGroups>
            <layers>
                <li>Belt</li>
            </layers>
            <defaultOutfitTags>
                <li>Soldier</li>
            </defaultOutfitTags>
            <tags>
                <li>Astrologer_Equipment</li>
            </tags>
        </apparel>
        <recipeMaker>
            <researchPrerequisite>LOF_Research_SeriesWeapon_AMA</researchPrerequisite>
            <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
            <workSkill>Crafting</workSkill>
            <effectWorking>Tailor</effectWorking>
            <soundWorking>Recipe_Tailor</soundWorking>
            <recipeUsers>
                <li>LOF_FabricationBench</li>
            </recipeUsers>
            <unfinishedThingDef>UnfinishedPack</unfinishedThingDef>
            <defaultIngredientFilter>
                <categories>
                    <li>Root</li>
                </categories>
                <disallowedThingDefs>
                    <li>Gold</li>
                    <li>Silver</li>
                    <li>Plasteel</li>
                    <li>Jade</li>
                    <li>Uranium</li>
                    <li MayRequire="Ludeon.RimWorld.Anomaly">Bioferrite</li>
                </disallowedThingDefs>
            </defaultIngredientFilter>
        </recipeMaker>
    </ThingDef>

    <ThingDef ParentName="Astro_ApparelBase">
        <defName>LOF_Equip_Waistbag</defName>
        <label>埃兰迪尔</label>
        <description>挂在腰间的装备，消耗洞察力可以进行折跃距离较长.</description>
        <graphicData>
            <texPath>Things/Apparel/LOF_Waistbag/Waistbag</texPath>
            <graphicClass>Graphic_Single</graphicClass>
        </graphicData>
        <possessionCount>1</possessionCount>
        <!--占位,原版数值-->
        <statBases>
            <MaxHitPoints>120</MaxHitPoints>
            <WorkToMake>8000</WorkToMake>
            <Mass>2</Mass>
            <EquipDelay>3.0</EquipDelay>
        </statBases>
        <equippedStatOffsets>
        </equippedStatOffsets>
        <comps>
            <li Class="AKA_Ability.TCP_AKATracker">
                <trackerGenProp>
                    <abilities>
                        <li>LOF_Ability_Teleport</li>
                    </abilities>
                </trackerGenProp>
            </li>
        </comps>
        <costList>
            <Cloth>20</Cloth>
            <LOF_Thing_StarlightIron>40</LOF_Thing_StarlightIron>
            <ComponentIndustrial>10</ComponentIndustrial>
        </costList>
        <apparel>
            <wornGraphicPath>Things/Apparel/LOF_Waistbag/Waistbag</wornGraphicPath>
            <bodyPartGroups>
                <li>Waist</li>
            </bodyPartGroups>
            <layers>
                <li>LOF_ApparelLayer_Webbing</li>
            </layers>
            <defaultOutfitTags>
                <li>Soldier</li>
            </defaultOutfitTags>
            <tags>
                <li>Astrologer_Equipment</li>
            </tags>
        </apparel>
        <recipeMaker Inherit="false">
            <!--researchPrerequisite>LOF_Research_AdvancedManufacture</researchPrerequisite>
            <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
            <workSkill>Crafting</workSkill>
            <effectWorking>Tailor</effectWorking>
            <soundWorking>Recipe_Tailor</soundWorking>
            <recipeUsers>
                <li>ElectricTailoringBench</li>
                <li>HandTailoringBench</li>
            </recipeUsers>
            <unfinishedThingDef>UnfinishedPack</unfinishedThingDef-->
        </recipeMaker>
    </ThingDef>

</Defs>